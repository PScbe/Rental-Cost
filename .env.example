# Meta Ads Configuration
# Get your access token from: https://developers.facebook.com/tools/accesstoken/
VITE_META_ACCESS_TOKEN=your_meta_access_token_here

# Meta Pixel ID (found in Meta Events Manager)
VITE_META_PIXEL_ID=your_pixel_id_here

# Meta Ad Account ID (format: act_123456789)
VITE_META_AD_ACCOUNT_ID=your_ad_account_id_here

# Optional: Test Event Code for testing events before going live
VITE_META_TEST_EVENT_CODE=
